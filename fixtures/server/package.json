{
    "name": "fixture-elementql-server",
    "version": "0.0.1",
    "description": "",
    "keywords": [
        "elementql",
        "server",
        "fixture"
    ],
    "main": "build/index.js",
    "module": "build/index.es5.js",
    "typings": "build/types/index.d.ts",
    "files": [
        "build"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com/)",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/elementql"
    },
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "prebuild": "rimraf build",
        "build": "rollup -c rollup.config.ts",
        "start": "rollup -c rollup.config.ts -w",
        "spec": "jest ./src"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "src/*.{js,ts}"
        ]
    },
    "dependencies": {
        "@plurid/elementql-server": ">=0",
        "@plurid/elementql-tag": ">=0"
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.11.1",
        "coveralls": "^3.0.11",
        "cross-env": "^7.0.2",
        "jest": "^25.3.0",
        "jest-config": "^25.3.0",
        "lodash.camelcase": "^4.3.0",
        "prompt": "^1.0.0",
        "replace-in-file": "^5.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.3.4",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "ts-jest": "^25.3.1",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    }
}
